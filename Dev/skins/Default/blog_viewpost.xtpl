<!-- BEGIN: BlogPost -->
<div id="main">
 <div class="article">
  <h2 class="title"><img src="{icon}" style="float:left" alt="" />{subject}{unpublished}</h2>
  <span class="postbit_top"></span>
  <div class="rounded_top" style="background-position:22px 0px;">
   <span class="one"></span>
   <span class="two"></span>
   <span class="three"></span>
   <span class="four"></span>
  </div>

  <div class="postbit">
<!-- BEGIN: Image -->
   <img class="float" src="{image}" alt="" />
<!-- END: Image -->
   {text}
   <p></p>
  </div>

  <div class="rounded_bottom">
   <span class="one">&nbsp;</span>
   <span class="two">&nbsp;</span>
   <span class="three">&nbsp;</span>
   <span class="four">&nbsp;</span>
  </div>
 </div>

<!-- BEGIN: NavLinks -->
  <div class="article">
   <div style="float:left">{older}</div>
   <div style="float:right">{newer}</div>
   <p></p>
  </div>
<!-- END: NavLinks -->

 <div class="article">
  {delicious}
  {stumbleupon}
  {reddit}
  {gplus}
  {facebook}
  {twitter}
  Posted on {date} by {post_author} in: {cat_text} | {count} comment(s) {closed}
  {mod_controls}
 </div>

<!-- BEGIN: Comments -->
  <div class="article">
   <div class="title"><a id="comments"></a>Comments</div>
  </div>
  {comments}
<!-- END: Comments -->

 {comment_form}
</div>
{sidebar}
<!-- END: BlogPost -->